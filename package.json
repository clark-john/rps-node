{
  "name": "rps-node",
  "version": "1.0.0",
  "description": "Rock Paper Scissors w/ Node",
  "author": "Clark",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build-start": "tsc && node dist/src/index.js",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "watch": "tsc -w",
    "watch2": "nodemon dist/src/index.js",
    "lint": "eslint ./src --ignore-pattern .gitignore",
    "migrate": "prisma migrate dev --name",
    "studio": "cross-env BROWSER=none prisma studio",
    "resetdb": "prisma migrate reset",
    "test": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.14.0",
    "casual": "^1.6.2",
    "colorette": "^2.0.17",
    "cross-env": "^7.0.3",
    "file-exists": "^5.0.1",
    "hidefile": "^3.0.0",
    "is-windows": "^1.0.2",
    "moment": "^2.29.3",
    "prisma": "^3.14.0",
    "prompts": "^2.4.2",
    "random": "^3.0.6",
    "sha1": "^1.1.1",
    "slugify": "^1.6.5",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@types/file-exists": "^5.0.0",
    "@types/hidefile": "^3.0.0",
    "@types/is-windows": "^1.0.0",
    "@types/node": "^17.0.40",
    "@types/prompts": "^2.0.14",
    "@types/sha1": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "husky": "^8.0.1",
    "jest": "^28.1.1",
    "nodemon": "^2.0.16",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
